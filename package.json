{
  "name": "@js-widgets/widget-registry-boilerplate",
  "description": "Boilerplate code to create your own widget registry.",
  "private": false,
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "test": "node ./scripts/download-registry.js && craco test --watchAll=false",
    "compile-widgets": "node scripts/ci/compile-widgets.js",
    "start": "node ./scripts/download-registry.js && craco start",
    "build": "craco build",
    "eject": "craco eject",
    "lint:fix": "eslint --fix . && prettier --write .",
    "lint": "eslint . && prettier --check ."
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:js-widgets/widget-registry-boilerplate.git"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^6.1.3",
    "@craco/craco": "^6.0.0",
    "ajv-cli": "^3.0.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2"
  },
  "dependencies": {
    "@carbon/icons-react": "^10.22.0",
    "@carbon/themes": "^10.24.0",
