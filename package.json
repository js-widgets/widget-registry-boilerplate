    "@testing-library/user-event": "^12.1.10",
    "carbon-components": "^10.24.0",
    "carbon-components-react": "^7.24.0",
    "carbon-icons": "^7.0.7",
    "config": "^3.3.1",
    "fuse.js": "^6.4.3",
    "ncp": "^2.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "rimraf": "^3.0.2",
    "sass": "^1.30.0",
    "web-vitals": "^0.2.4"
  }
